{
  "name": "keybr_colemak-dh",
  "version": "0.1.0",
  "description": "Simple script to change the Colemak layout to Colemak-DH on Keybr.com",
  "author": "Michele Bianchi",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "rollup -wc rollup.conf.js",
    "clean": "del-cli dist types",
    "build:js": "rollup -c rollup.conf.js",
    "prebuild": "run-s ci clean",
    "build": "cross-env NODE_ENV=production run-s build:js",
    "ci": "run-s lint",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "@violentmonkey/dom": "^2.0.2",
    "@violentmonkey/ui": "^0.7.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.0",
    "@gera2ld/plaid": "~2.4.0",
    "@gera2ld/plaid-common-ts": "~2.4.0",
    "@gera2ld/plaid-rollup": "~2.4.0",
    "del-cli": "^4.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.4.1",
    "rollup-plugin-userscript": "^0.1.1"
  }
}
